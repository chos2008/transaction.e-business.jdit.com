<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
  "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="org.chos.transaction.DocumentPart" alias="DocumentPart"/>
		<typeAlias type="org.chos.transaction.Bid" alias="Bid"/>
		<typeAlias type="org.chos.transaction.Requirement" alias="Requirement"/>
		<typeAlias type="org.chos.transaction.User" alias="User"/>
		<typeAlias type="org.chos.transaction.passport.Session" alias="Session"/>
		<typeAlias type="org.chos.transaction.passport.OAuthSession" alias="OAuthSession"/>
	</typeAliases>
		

	<mappers>
		<mapper resource="ibatis/document.xml" />
		<mapper resource="ibatis/User.xml" />
		<mapper resource="ibatis/Requirement.xml" />
		<mapper resource="ibatis/Bid.xml" />
		<mapper resource="ibatis/Session.xml" />
		<mapper resource="ibatis/Auth-Session.xml" />
	</mappers>
	
</configuration>
